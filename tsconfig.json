{
    "compilerOptions": {
        "lib": [
            "ESNext",
            "dom"
        ],
        "target": "ESNext",
        "module": "ESNext",
        "rootDir": ".",
        "moduleResolution": "Bundler",
        "moduleDetection": "force",
        "allowJs": true,
        "strict": true,
        "skipLibCheck": true,
        "noFallthroughCasesInSwitch": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noPropertyAccessFromIndexSignature": true,
    }
}
